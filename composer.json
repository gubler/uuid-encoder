{
    "name": "gubler/uuid-encoder",
    "type": "library",
    "description": "Tool for encoding ramsey/uuid into shorter strings",
    "keywords": [
        "uuid"
    ],
    "homepage": "https://github.com/gubler/uuid-encoder",
    "license": "MIT",
    "authors": [
        {
            "name": "Daryl Gubler",
            "email": "daryl@dev88.co",
            "homepage": "http://dev88.co"
        }
    ],
    "support": {
        "issues": "https://github.com/gubler/uuid-encoder/issues",
        "source": "https://github.com/gubler/uuid-encoder"
    },
    "require": {
        "php": "^7.2.5 || ^8.0",
        "brick/math": "^0.9",
        "ramsey/uuid": "^3.0 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Gubler\\UuidEncoder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gubler\\UuidEncoder\\Tests\\": "tests/"
        }
    }
}
